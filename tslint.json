{
    "defaultSeverity": "error",
    "extends": "tslint:recommended",
    "jsRules": {},
    "rules": {
        "quotemark": false,
        "semicolon": [ true, "never" ],
        "curly": false,
        "arrow-parens": false,
        "trailing-comma": [ true, "never" ],
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "no-console": false,
        "one-line": false,
        "max-line-length": false,
        "no-consecutive-blank-lines": false
    },
    "rulesDirectory": [],
    "exclude": [
        "./src/object.ts"
    ]
}
